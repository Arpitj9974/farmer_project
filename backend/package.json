{
  "name": "agricultural-marketplace-backend",
  "version": "1.0.0",
  "description": "Agricultural B2B Bulk Trading Platform Backend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node database/runSeed.js",
    "migrate": "node database/migrate.js up",
    "migrate:down": "node database/migrate.js down",
    "migrate:status": "node database/migrate.js status",
    "migrate:create": "node database/migrate.js create",
    "health": "node -e \"const http=require('http');http.get('http://localhost:5002/api/health',r=>{let d='';r.on('data',c=>d+=c);r.on('end',()=>{console.log(JSON.parse(d));process.exit(r.statusCode===200?0:1)})}).on('error',e=>{console.error('Server not running');process.exit(1)})\""
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.13.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "winston": "^3.19.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}